<!DOCTYPE html>
<html lang="en">
<head>
<title>sーDELI_regist</title>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="aStar Fashion Template Project">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" type="text/css" href="styles/bootstrap-4.1.3/bootstrap.css">
<link href="plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="plugins/OwlCarousel2-2.2.1/owl.carousel.css">
<link rel="stylesheet" type="text/css" href="plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
<link rel="stylesheet" type="text/css" href="plugins/OwlCarousel2-2.2.1/animate.css">
<link rel="stylesheet" type="text/css" href="styles/checkout.css">
<link rel="stylesheet" type="text/css" href="styles/checkout_responsive.css">
<link rel="stylesheet" type="text/css" href="styles/navbar.css">
</head>
<body>
<input type="hidden" name="entry_url" id='entry_url' value="{{constant('member\\Bootstrap::ENTRY_URL')}}">

<div class="super_container">

	<!-- Sidebar -->

  {% include 'sidebar.html.twig' %}
  {% include 'navbar.html.twig' %}

	<!-- Home -->

	<div class="home">
		<div class="parallax_background parallax-window" data-parallax="scroll" data-image-src="images/header.png" data-speed="0.8"></div>
		<div class="home_container">
			<div class="home_content">
				<div class="home_title">Checkout</div>
				<div class="breadcrumbs">
					<ul class="d-flex flex-row align-items-center justify-content-start">
						<li>会員登録ページ</li>
					</ul>
				</div>
			</div>
		</div>
	</div>

	<!-- Checkout -->

	<div class="checkout">
		<div class="section_container">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="checkout_container d-flex flex-xxl-row flex-column align-items-start justify-content-start">

							<!-- Billing -->
							<div class="billing checkout_box">
								<div class="checkout_title">会員情報</div>
								<div class="checkout_form_container">
									<form action="confirm.php"  class="checkout_form" method="post">
                    <div>
											<!-- Name -->
											<label for="checkout_company">お名前</label>
											<input type="text"  class="checkout_input" name="name" value="{{dataArr.name}}">
                      {% if errArr.name | length > 0 %}<br><span class="text-danger">*{{errArr.name}}</span>{% endif %}
										</div>
                    <div class="row">
											<div class="col-lg-6">
												<!-- zip1 -->
												<label for="checkout_name">郵便番号</label>
												<input type="text"  name="zip1" class="checkout_input" value="{{dataArr.zip1}}">
                        {% if errArr.zip1 | length > 0 %}<br><span class="text-danger">*{{errArr.zip1}}</span>{% endif %}
											</div>
											<div class="col-lg-6">
												<!-- zip2 -->
												<label for="checkout_last_name">　</label>
												<input type="text"  name="zip2" class="checkout_input" value="{{dataArr.zip2}}">
                        {% if errArr.zip2 | length > 0 %}<br><span class="text-danger">*{{errArr.zip2}}</span>{% endif %}
											</div>
										</div>
                    <div>
											<!-- address -->
											<label for="checkout_company">住所</label>
											<input type="text"  class="checkout_input" name="address" value="{{dataArr.address}}">
                      {% if errArr.address | length > 0 %}<br><span class="text-danger">*{{errArr.address}}</span>{% endif %}
										</div>
                    <div>
											<!-- email -->
											<label for="checkout_company">メールアドレス</label>
											<input type="text"  class="checkout_input" name="email" value="{{dataArr.email}}">
                      {% if errArr.email | length > 0 %}<br><span class="text-danger">*{{errArr.email}}</span>{% endif %}
										</div>
                    <div class="row">
											<div class="col-lg-4">
												<!-- tel1 -->
												<label for="checkout_name">電話番号</label>
												<input type="text"  class="checkout_input" name="tel1" value="{{dataArr.tel1}}">
                        {% if errArr.tel1 | length > 0 %}<br><span class="text-danger">*{{errArr.tel1}}</span>{% endif %}
											</div>
											<div class="col-lg-4">
												<!-- tel2 -->
												<label for="checkout_last_name">　</label>
												<input type="text"  class="checkout_input" name="tel2" value="{{dataArr.tel2}}">
                        {% if errArr.tel2 | length > 0 %}<br><span class="text-danger">*{{errArr.tel2}}</span>{% endif %}
											</div>
											<div class="col-lg-4">
												<!-- tel3 -->
												<label for="checkout_last_name">　</label>
												<input type="text"  class="checkout_input" name="tel3" value="{{dataArr.tel3}}">
                        {% if errArr.tel3 | length > 0 %}<br><span class="text-danger">*{{errArr.tel3}}</span>{% endif %}
											</div>
										</div>
                    <div>
											<!-- password -->
											<label for="checkout_company">パスワード</label>
											<input type="text"  class="checkout_input" name="password" value="{{dataArr.password}}">
                      {% if errArr.password | length > 0 %}<br><span class="text-danger">*{{errArr.password}}</span>{% endif %}
										</div>
                    <div><input class="checkout_button trans_200" type="submit" name="confirm" value="登録確認" /></div>
									</form>
								</div>
							</div>

						</div>
					</div>
				</div>
			</div>
		</div>
	</div>



	<!-- Footer -->

  {% include 'footer.html.twig' %}

</div>

<script src="js/jquery-3.2.1.min.js"></script>
<script src="styles/bootstrap-4.1.3/popper.js"></script>
<script src="styles/bootstrap-4.1.3/bootstrap.min.js"></script>
<script src="plugins/greensock/TweenMax.min.js"></script>
<script src="plugins/greensock/TimelineMax.min.js"></script>
<script src="plugins/scrollmagic/ScrollMagic.min.js"></script>
<script src="plugins/greensock/animation.gsap.min.js"></script>
<script src="plugins/greensock/ScrollToPlugin.min.js"></script>
<script src="plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
<script src="plugins/easing/easing.js"></script>
<script src="plugins/parallax-js-master/parallax.min.js"></script>
<script src="js/checkout.js"></script>
</body>
</html>
